generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model DataCollection {
  id            Int @id @default(autoincrement())
  bpm           Float
  spo           Float
  suhu          Float
  createdAt     DateTime @default(now())
}

model KirimEsp {
  id            Int @id @default(autoincrement())
  currentstate  Int
  delay         Int
}
